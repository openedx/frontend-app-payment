    "@stripe/stripe-js": "^1.36.0",
    "axios": "^0.27.2",
    "bootstrap": "4.6.1",
    "classnames": "^2.3.1",
    "core-js": "^3.23.5",
    "form-urlencoded": "^6.0.6",
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1",
    "lodash.without": "^4.4.0",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
